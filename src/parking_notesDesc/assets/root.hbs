<div class="alertBox" id='firstLoginTip'>
  <div class='alert'>
    <div class="content"></div>
    <div class="operate">
      <input class="btn one sm" id='firstLoginTipBtn' type="button" value="确认">
    </div>
  </div>
</div>

<div class='errBox' style='display:none;'>
  <div class='g-autoBoxOut'>
    <div class='g-autoBoxIn content'></div>
  </div>
</div>

<div class="g-loading" style='display:none;'>
  <span><!--<i class='iconfont icon-load'></i>--><img src='http://timg.ffan.com/convert/resize/url_T1dgdTBTLQ1RCvBVdK/tfs/0f2b063593aa7a368fcf307d406204fa.png'>加载中</span>
</div>

<div class='notWork'>
  <p><i class='iconfont icon-notwork'></i></p>
