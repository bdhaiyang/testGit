<div id="wrapper">
  <div id="scroller">
    <div id="pullDown">
      <span class="pullDownIcon"></span><span class="pullDownLabel">...</span>
    </div>
    <section class="parkNoteDesc">
  <ul>
    <li>
      <span>停车总金额</span>
      <span class="fr red">&yen;{{money}}</span>
    </li>
  </ul>
  <ul>
    <li>
      <span>车牌号码</span>
      <span class="fr">{{carLicense}}</span>
    </li>
    <li>
      <span>停车场</span>
      <span class="fr">{{plazaName}}</span>
    </li>
    <li>
      <span>入场时间</span>
      <span class="fr">{{inTime}}</span>
    </li>
    <li>
      <span>出场时间</span>
      <span class="fr">{{outTime}}</span>
    </li>
    <li>
      <span>停车时长</span>
      <span class="fr">{{parkingTime}}</span>
    </li>
  </ul>
  <ul>
    <li class='bottomBor'>
      <span>优惠金额</span>
      <span class="fr">- &yen;{{discountMoney}}</span>
    </li>
    <li>
      <span>{{activityName}}</span>
      <span class="fr">- &yen;{{reduceMoney}}</span>
    </li>
    <li>
      <span>{{couponName}}</span>
      <span class="fr">- &yen;{{couponMoney}}</span>
    </li>
  </ul>
  <ul class='money'>
    <li>
      <p class="fr">{{moneyName}}<span class="red" id="money">{{payMoney}}</span></p>
    </li>
  </ul>
  <div class="operation">
    <input type="button" class="btn btnBlue" value="付停车费" id="payBtn">
  </div>
</section>
    <div id="pullUp" style='display:none;'>
      <span class="pullUpIcon"></span><span class="pullUpLabel">加载更多...</span>
    </div>
  </div>
</div>

<footer class="g-footer">
  <nav class="g-nav">
    <a href="{{wechatUrl}}">
      <span class="icon"><i class="iconfont icon-car"></i></span>
      <span class="icon-desc">停车缴费</span>
    </a>
    <a href="/fe/fe/sea-parking/html/parking_carList.html">
      <span class="icon"><i class="iconfont icon-wangfuguanlichepai"></i></span>
